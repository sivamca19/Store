<table border="1px" width="100%" class="admin_users_table">
<th>Email</th>
<th>Role</th>
<th>Actions</th>
<%@users.each do |user|%>
<tr>
<td><%=user.email%></td>
<td><%=user.name%></td>
<td>
  <%=link_to "Edit", edit_admin_user_path(user)%>
  <%=link_to "Show", admin_user_path(user)%>
  <%=link_to "Delete", admin_user_path(user), :onclick=>"return confirm('Are you sure you want to delete?')", :method => :delete%>
</td>
</tr>
<%end%>
</table>
